function uid(){return Date.now().toString(36)+Math.random().toString(36).slice(2);}

async function startUpload(){
  const log = document.getElementById('log');
  log.innerHTML = '';
  let ok=0, fail=0;
  for(const b of BLOGGERS){
    try{
      await db.collection('existingBloggers').doc(uid()).set(b);
      log.innerHTML += `<span class="ok">✓ ${b.nick||b.name||'(닉네임없음)'} - ${b.history.length}건 이력</span>\n`;
      ok++;
    }catch(e){
      log.innerHTML += `<span class="err">✗ ${b.nick||b.name} 실패: ${e.message}</span>\n`;
      fail++;
    }
  }
  log.innerHTML += `\n완료: ${ok}명 성공, ${fail}명 실패`;
}
</script>
